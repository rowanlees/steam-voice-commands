<Window x:Class="SVC.WPF.Views.InstalledGamesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SVC.WPF.Views"
        mc:Ignorable="d"
        Title="Installed Games" Height="400" Width="600"
        Style="{DynamicResource {x:Type Window}}"
        >
    <Grid>
        <ListBox x:Name="GamesListBox" DisplayMemberPath="GameName" Margin="10"/>
    </Grid>
</Window>
